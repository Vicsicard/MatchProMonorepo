{
  "name": "matchpro-career-suite",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "dev": "yarn workspace matchproresumewebsite start",
    "build": "node scripts/vercel-build.js",
    "build:main": "yarn workspace matchproresumewebsite build",
    "build:libs": "yarn workspace @matchpro/ui build && yarn workspace @matchpro/data build && yarn workspace @matchpro/styles build && yarn workspace @matchpro/config build",
    "lint": "yarn workspaces run lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rimraf ./apps/*/node_modules ./libs/*/node_modules ./node_modules",
    "vercel-build": "node scripts/vercel-build.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.46.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.7.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.32",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.0",
    "tsup": "^8.0.1",
    "typescript": "^5.0.2",
    "vite": "^5.0.8"
  },
  "resolutions": {
    "typescript": "^5.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
