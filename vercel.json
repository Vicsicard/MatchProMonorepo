{
  "version": 2,
  "buildCommand": "yarn build",
  "installCommand": "yarn install",
  "outputDirectory": ".vercel/output/static",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/resume-builder/(.*)",
      "destination": "/resume-builder/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
